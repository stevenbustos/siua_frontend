salSelection/\")\n      .then(response => {\n        for (var k in response.data.data){\n          var causal= {}\n          causal[\"_id\"] = response.data.data[k]._id;\n          causal[\"name\"] = response.data.data[k].nombre\n          this.causals.push(causal);\n        }\n      })\n    })\n\n  },\n  methods:{\n    optionsSelect(item){\n      console.log(this.personasSelect);\n      delete this.personasSelect[item.value];\n      console.log(this.personasSelect);\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components/requests"}]}