n</template>\n\n<script>\nimport {axios} from '../../helpers/axios';\n\nvar headersDic = {};\nheadersDic[process.env.VUE_APP_KEY_HEADER_NAME] = process.env.VUE_APP_KEY_HEADER_CONTENT;\nheadersDic[process.env.VUE_APP_USER_HEADER_NAME] = \"test1\";\nheadersDic[process.env.VUE_APP_PASS_HEADER_NAME] = \"test123\";\nheadersDic[process.env.VUE_APP_MODULE_HEADER_NAME] = \"auth\";\n\nexport default{\n\n  data() {\n    return {\n      info: null\n    }\n  },\n  mounted () {\n    axios\n      .get('http://35.237.249.152:8000', {\n        headers: headersDic\n      })\n      .then(response => (this.info = response))\n  },\n  // methods: {\n  //   login: function(){\n  //     axios\n  //     .get('https://api.coindesk.com/v1/bpi/currentprice.json')\n  //     .then(response => (this.info = response))\n  //   }\n  // }\n}\n</script>\n"],"sourceRoot":"src/components/login"}]}