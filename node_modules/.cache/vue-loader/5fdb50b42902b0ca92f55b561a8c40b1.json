n          }\n        ],\n        numero_ORPA:\"\",\n        solicitud:this.$route.params.id\n      }\n    },\n    addFormPay() {\n      let newObject = {\n        numero:\"\",\n        valor:\"\",\n        fecha:\"\",\n        planillas:[\n          {\n            planilla_numero:\"\",\n            planilla_fecha:\"\"\n          }\n        ],\n        numero_ORPA:\"\"\n      }\n      this.payments.push(newObject)\n    },\n    saveContinue(){\n      axios\n      .post(API_IP+\"/payment/\", this.payments )\n      .then(response => {\n        console.log(response);\n        this.$swal({\n          title: 'Exito!',\n          text: 'Se crearon los pagos correctamente',\n          type: 'success',\n          confirmButtonText: 'Aceptar'\n        }).then((result) => {\n          if (result.value) {\n            this.$router.push({ name: 'payments'});\n          }\n        });\n      }).catch(error => {\n        console.log(error.response);\n        this.$swal({\n          title: 'Error!',\n          text: error.response.data,\n          type: 'error',\n          confirmButtonText: 'Cerrar'\n        });\n      })\n    }\n  }\n}\n</script>\n"]}]}