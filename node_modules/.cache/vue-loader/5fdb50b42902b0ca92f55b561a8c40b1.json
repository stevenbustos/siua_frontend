eadcrumb\">\n          <ol class=\"breadcrumb\">\n            <li class=\"breadcrumb-item\"><a href=\"/\">Home</a></li>\n            <li class=\"breadcrumb-item active\" aria-current=\"page\">Pagos</li>\n          </ol>\n        </nav>\n      </div>\n      <div class=\"row\">\n        <div class=\"col-md-10 offset-md-1\" style=\"margin-bottom:80px;\">\n          <div class=\"row\">\n            <div class=\"col\">\n              <button onclick=\"window.history.back();\" class=\"btn btn-primary\" style=\"background:#003e1e;border-color:#003e1e;\">\n                <font-awesome-icon icon=\"arrow-left\" size=\"lg\"></font-awesome-icon>\n              </button>\n            </div>\n          </div>\n          <div>&nbsp;</div>\n          <modal name=\"update-pay-variable\" :adaptive=\"true\" :scrollable=\"true\" width=\"50%\" height=\"auto\">\n            <update-pay-variable\n            :hide_modal=\"modal_show\"\n            @modalShow=\"closeModalUpdatePayVaribale\"\n            :id_pay=\"id_pay\"\n            ></update-pay-variable>\n          </modal>\n          <div class=\"row justify-content-center\">\n            <div class=\"col-5 align-self-center\">\n              <form>\n                <div>\n                  <div class=\"row\">\n                    <div class=\"col\">\n                      <label for=\"personName\">Valor:</label>\n                      <div class=\"form-group\">\n                        <input v-model=\"pay.valor\" type=\"number\" class=\"form-control\" id=\"payment_value\" aria-describedby=\"paymentValue\" placeholder=\"Valor\">\n                      </div>\n                    </div>\n                    <div class=\"col\">\n                      <label for=\"personName\">Fecha:</label>\n                      <div class=\"form-group\">\n                        <input v-model=\"pay.fecha\" type=\"date\" class=\"form-control\" id=\"payment_date\" aria-describedby=\"paymentDate\">\n                      </div>\n                    </div>\n                  </div>\n                </div>\n                <div>\n                  <div class=\"row\">\n                    <div class=\"col\">\n                      <label for=\"personName\">No. de planilla:</label>\n                      <div class=\"form-group\">\n                        <input v-model=\"pay.planillas[0].planilla_numero\" type=\"number\" class=\"form-control\" id=\"payment_planillanumber\" aria-describedby=\"paymentPlanillanumber\" placeholder=\"Planilla\">\n                      </div>\n                    </div>\n                    <div class=\"col\">\n                      <label for=\"personName\">Fecha de pago de planilla:</label>\n                      <div class=\"form-group\">\n                        <input v-model=\"pay.planillas[0].planilla_fecha\" type=\"date\" class=\"form-control\" id=\"payment_planilladate\" aria-describedby=\"paymentPlanilladate\">\n                      </div>\n                    </div>\n                  </div>\n                </div>\n                <div>\n                  <div class=\"row justify-content-center\">\n                    <div class=\"col-6\">\n                      <label for=\"personName\">ORPA:</label>\n                      <div class=\"form-group\">\n                        <input v-model=\"pay.numero_ORPA\" type=\"number\" class=\"form-control\" id=\"payment_ORPA\" aria-describedby=\"paymentORPA\" placeholder=\"ORPA\">\n                      </div>\n                    </div>\n                  </div>\n                </div>\n                <div>&nbsp;</div>\n                <div class=\"row justify-content-center\">\n                  <div class=\"col-4 text-center\">\n                    <button class=\"btn btn-primary\" type=\"button\" v-on:click=\"addPay()\" style=\"background:#003e1e;border-color:#003e1e;\">\n                      <font-awesome-icon icon=\"pencil-alt\" size=\"lg\"></font-awesome-icon>\n                      Agregar\n                    </button>\n   