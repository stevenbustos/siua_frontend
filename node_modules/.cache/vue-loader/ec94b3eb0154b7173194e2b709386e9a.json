el>\n            <multiselect v-model=\"value\" :options=\"options\" :multiple=\"true\" group-values=\"libs\" group-label=\"language\" :group-select=\"true\" placeholder=\"Type to search\" track-by=\"name\" label=\"name\"><span slot=\"noResult\">Oops! No elements found. Consider changing the search query.</span></multiselect>\n            <pre class=\"language-json\"><code>{{ value  }}</code></pre>\n          </div> -->\n          <!-- {{module}} -->\n\n          <div class=\"form-group\">\n            <label for=\"rolModules\">Modulos del rol:</label>\n            <multiselect v-model=\"rol_modules\" :options=\"modules\" :multiple=\"true\" :close-on-select=\"false\" :clear-on-select=\"false\" :hide-selected=\"true\" :preserve-search=\"true\" label=\"name\" track-by=\"_id\" :preselect-first=\"true\">\n            </multiselect>\n            <!-- <v-select id=\"rolModules\" multiple v-model=\"rol_modules\" :options=\"module_name\"></v-select> -->\n          </div>\n          {{rol_modules}}\n          {{mod_id}}\n          <!-- {{modules}} -->\n          <div class=\"form-group\">\n            <label for=\"rolStates\">Estado del rol:</label>\n            <v-select id=\"rolStates\" v-model=\"rol_state\" :options=\"states\"></v-select>\n          </div>\n          <div class=\"form-group\">\n            <label for=\"permitDescription\">Descripción:</label>\n            <textarea v-model=\"description\" class=\"form-control\" aria-label=\"permitDescription\"\n            placeholder=\"Descripción\" :rows=\"6\" :max-rows=\"10\"></textarea>\n          </div>\n          <button class=\"btn btn-primary\" @click=\"submit()\" style=\"background:#003e1e;border-color:#003e1e;\">\n            <font-awesome-icon icon=\"pencil-alt\" size=\"lg\"></font-awesome-icon>\n            Crear nuevo\n          </button>\n        </form>\n      </div>\n    </div>\n  </div>\n</div>\n</div>\n</div>\n</template>\n\n<script>\nimport Multiselect from 'vue-multiselect'\nconst axios = require('axio